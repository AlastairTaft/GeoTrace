{
  "expo": {
    "name": "geo-trace",
    "icon": "./assets/images/logo.png",
    "version": "0.1.0",
    "slug": "geo-trace",
    "sdkVersion": "37.0.0",
    "ios": {
      "icon": "./assets/images/icon-ios.png",
      "bundleIdentifier": "me.alastairtaft.geotrace",
      "buildNumber": "0.0.0",
      "supportsTablet": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow use of your location to track community COVID-19 spread.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow use of your location to track community COVID-19 spread.",
        "NSLocationAlwaysUsageDescription": "Allow use of your location to track community COVID-19 spread.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "me.alastairtaft.geotrace",
      "versionCode": 1
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {}
        }
      ]
    }
  }
}
